<listing xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:module:Magento_Ui:etc/ui_configuration.xsd">
    <columns name="sales_order_columns">
        <column name="tracking_code" class="ChilexpressOficial\Chilexpress\Ui\Component\Listing\Column\TrackingCode">
            <argument name="data" xsi:type="array">
                <item name="config" xsi:type="array">
                    <item name="bodyTmpl" xsi:type="string">ui/grid/cells/html</item>
                    <item name="disableAction" xsi:type="boolean">true</item>
                    <!--item name="component" xsi:type="string">ChilexpressOficial_Chilexpress/trackinglink</item-->
                    <item name="label" xsi:type="string" translate="true">Tracking Code</item>
                </item>
            </argument>
        </column>
         <column name="tracking_actions" class="ChilexpressOficial\Chilexpress\Ui\Component\Listing\Column\TrackingActions">
            <argument name="data" xsi:type="array">
                <item name="config" xsi:type="array">
                    <item name="bodyTmpl" xsi:type="string">ui/grid/cells/html</item>
                    <item name="disableAction" xsi:type="boolean">true</item>
                    <item name="label" xsi:type="string" translate="true">Tracking Actions</item>
                </item>
            </argument>
        </column>
    </columns>
</listing>